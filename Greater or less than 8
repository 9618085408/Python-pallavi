num1 = int(input("Enter the first number: "))
num2 = int(input("Enter the second number: "))

if num1 > 8:
  print(f"{num1} is greater than 8")
elif num1 < 8:
  print(f"{num1} is less than 8")
else:
  print(f"{num1} is equal to 8")

if num2 > 8:
  print(f"{num2} is greater than 8")
elif num2 < 8:
  print(f"{num2} is less than 8")
else:
  print(f"{num2} is equal to 8")
